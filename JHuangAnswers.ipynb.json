    "import numpy as np\n",
    "from pylab import *\n",
    "from scipy import stats\n",
    "from scipy.optimize import curve_fit\n",
    "% matplotlib inline\n",
    "from matplotlib import rc\n",
    "rc('text', usetex=True)\n",
    "from sklearn import datasets, linear_model\n",
    "\n",
    "\n",
    "# plot function 4\\pi^2L = gT^2, the slope is g.\n",
    "\n",
    "x=[0.25,1,2.25,4,6.25,9,12.25,16,20.25]\n",
    "\n",
    "y=[0.394784176043574,0.986960440108936,2.76348923230502,3.94784176043574,5.52697846461004,9.08003604900221,12.2383094573508,13.8174461615251,19.7392088021787]\n",
    "\n",
    "(m,b)=polyfit(x,y,1)\n",
    "print(m)\n",
    "print(b)\n",
    "\n",
    "yp=polyval([m,b],x)\n",
    "plot(x,yp)\n",
    "scatter(x,y)\n",
    "grid(True)\n",
    "\n",
    "\n",
    "plt.title('length as a function of period squared')\n",
    "plt.ylabel('$\\mathrm{4\\pi^2L(m)}$')\n",
    "plt.xlabel('$\\mathrm{T^2 (s^2)}$')\n",
    "\n",
    "\n",
    "plt.errorbar(x, y, xerr=0.1, yerr=0.03)\n",
    "visible=True\n",
    "\n",
    "plt.show ()"
   ]
